pipeline {
    agent any
    stages {
        stage(‘main’) {
            steps {
                echo ’This is main branch’
  	 	}
     	}
        stage(‘sprint1’) {
            steps {
                echo ’This is sprint branch’
   	   	}
     	}
        stage(‘hotfix’) {
            steps {
                echo ’This is hot fix branch’
            }
        }
    }
}

